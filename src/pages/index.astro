---
export const prerender = true
import { getEntry } from 'astro:content'
import DocumentationLayout from '../layouts/DocumentationLayout.astro'
import { generateToc } from '../lib/toc'

const entry = await getEntry('getting-started', 'index')
const { Content, headings } = await entry.render()
const tocItems = generateToc(headings)
---

<DocumentationLayout title={entry.data.title} toc={tocItems}>
  <Content />
</DocumentationLayout>
